{
  "name": "react-final",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "custom": "node server.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test-execute": "yarn build && yarn start",
    "clean": "rimraf \"{.next,dist,build,out}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
    "lint": "eslint --rule \"@typescript-eslint/no-unused-vars: off\" \"**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "test": "jest -c test/jest.config.js --ci --reporters=default --reporters=jest-junit",
    "cypress:binary": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.7",
    "@fontsource/prata": "^4.5.5",
    "@fontsource/roboto": "^4.5.5",
    "@heroicons/react": "^1.0.6",
    "@material-ui/core": "^4.12.4",
    "@next/bundle-analyzer": "^12.1.4",
    "@reduxjs/toolkit": "^1.8.1",
    "@tailwindcss/forms": "^0.5.0",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^6.2.8",
    "js-cookies": "^1.0.4",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "next": "^12.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.0.0",
    "next-pwa": "^5.4.6",
    "next-seo": "^5.3.0",
    "nextjs-progressbar": "^0.0.14",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-content-loader": "^6.2.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.19",
    "react-redux": "^7.2.8",
    "react-toastify": "^8.2.0",
    "react-use": "^17.3.2",
    "sharp": "^0.30.3",
    "tailwindcss-rtl": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.20",
    "@svgr/cli": "^6.2.1",
    "@types/enzyme": "^3.10.11",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "cypress": "^9.5.3",
    "dotenv": "^16.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nock": "^13.2.4",
    "postcss": "^8.4.12",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
